@model IEnumerable<MvcMusicStore.Models.Genre>
@{
    ViewBag.Title = "Store";
}
<div class = "container">
    <div class = "row span12">

        <h3>Browse Genres</h3>

        <p>
            Select from @Model.Count() genres:</p>
        <ul>
            @foreach (var genre in Model)
            {
                <li>@Html.ActionLink(genre.Name, "details", new { genre = genre.GenreId })</li>
            }
        </ul>
    </div>
</div>